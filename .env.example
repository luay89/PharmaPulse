# PharmaPulse Environment Variables
# قم بنسخ هذا الملف إلى .env وأضف المفاتيح الخاصة بك

# مفتاح NewsAPI (احصل عليه من https://newsapi.org)
NEWS_API_KEY=your_newsapi_key_here

# إعدادات الخادم
PORT=3000
NODE_ENV=development

# إعدادات التخزين المؤقت (بالثواني)
CACHE_TTL_NEWS=1800
CACHE_TTL_DRUGS=3600

# عنوان الخادم
SERVER_URL=http://localhost:3000
